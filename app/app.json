{
    "expo": {
        "name": "FoldFlop",
        "slug": "foldflop",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "scheme": "foldflop",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#0D1117"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.foldflop.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#0D1117"
            },
            "package": "com.foldflop.app"
        },
        "web": {
            "bundler": "metro",
            "output": "single",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "supabaseUrl": "YOUR_SUPABASE_URL",
            "supabaseAnonKey": "YOUR_SUPABASE_ANON_KEY"
        }
    }
}